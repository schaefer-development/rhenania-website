		"fuse.js": "^6.5.3",
		"postcss-import": "^14.1.0",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.0",
		"svelte-hcaptcha": "^0.1.1",
		"svelte-preprocess": "^4.10.6",
