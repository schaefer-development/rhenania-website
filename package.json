		"algoliasearch": "^4.13.1",
		"graphql": "^16.3.0",
		"hcaptcha": "^0.1.1",
		"leaflet": "^1.7.1",
		"nodemailer": "^6.9.9",
		"svelte-i18n": "^3.4.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.13.12",
		"@graphql-codegen/introspection": "2.1.1",
		"@graphql-codegen/typescript": "2.4.8",
